{
    "name": "webcrawlerapi/sdk",
    "description": "A PHP SDK for WebCrawler API - turn website into data",
    "type": "library",
    "version": "1.0.7",
    "license": "MIT",
    "authors": [
        {
            "name": "WebcrawlerAPI Team"
        }
    ],
    "require": {
        "php": ">=8.1",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "guzzlehttp/psr7": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "WebCrawlerAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WebCrawlerAPI\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "keywords": [
        "webcrawler",
        "api",
        "sdk",
        "crawler",
        "scraper",
        "webcrawlerapi",
        "llm",
        "data",
        "website",
        "rag"
    ],
    "homepage": "https://github.com/webcrawlerapi/webcrawlerapi-php-sdk"
} 